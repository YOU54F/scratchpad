{
  "name": "transform",
  "version": "1.0.0",
  "description": "Transformation for change events from a postgres DB into firehose",
  "main": "delimiterLambda.js",
  "scripts": {
    "test": "jest",
    "test:it": "jest --config jest.config.integration.js",
    "build": "tsc",
    "docker:pg": "docker-compose up -d postgres",
    "deploy": "serverless deploy --stage local --verbose",
    "destroy": "serverless remove --stage local --verbose",
    "start": "NODE_OPTIONS=--max_old_space_size=4096 serverless offline start --host 0.0.0.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/pg": "^7.14.4",
    "aws-lambda": "^0.1.2",
    "bunyan": "^1.8.12",
    "moment": "^2.24.0",
    "pg": "^8.2.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.28",
    "@types/bunyan": "^1.8.6",
    "@types/jest": "^26.0.22",
    "@types/lolex": "^5.1.0",
    "@types/node": "^12.0.0",
    "@types/uuid": "^8.3.0",
    "decimal.js": "^10.1.1",
    "expect": "^25.2.7",
    "jest": "^26.6.3",
    "jest-diff": "^26.6.2",
    "jest-expect-contain-deep": "^1.0.1",
    "jest-matcher-utils": "^26.6.2",
    "lodash": "^4.17.15",
    "lolex": "^6.0.0",
    "serverless": "^3.22.0",
    "serverless-localstack": "^1.0.0",
